          }
        }
      }
    },
    "enums": {
      "root": "libs/enums",
      "sourceRoot": "libs/enums/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
